    </collision>

    <inertial>
      <mass value="10.0"/>
      <inertia ixx="0.45" iyy="0.45" izz="0.25" ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>

    <joint name="rr_wheel_joint" type="continuous">
      <parent link="base_link"/>
      <child  link="rr_wheel"/>
      <origin xyz="-0.9 -0.8 0.0"/>
    </joint>

</robot>
